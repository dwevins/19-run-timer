<div class="main-view">
  <div class="container">
    <ul class="run-list">
      {{#each model as |run|}}
        <li class="run-list__item" {{action 'goToReview' run on='click'}}>
          <p class="date run-list__item--date">{{run.raceDate}}</p>
          <p class="time run-list__item--time">{{run.runTime}}</p>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
<footer class="view-footer">
  <div class="container">
    <p class="view-footer__content">
      {{#link-to 'new-run' class='new-run-link'}}
        add new run
      {{/link-to}}
    </p>
  </div>
</footer>
{{outlet}}
